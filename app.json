{
  "expo": {
    "name": "expo_content_intent",
    "slug": "expo_content_intent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lemontech.expo_content_intent",
      "intentFilters": [
        {
          "action": "GET_CONTENT",
          "data": {
            "mimeType": "*/*"
          },
          "category": ["DEFAULT", "OPENABLE"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
